fun FizzBuzz (n) {
  for (var i = 1; i <= n; i = i + 1) {
    if (i % 3 == 0) {
      if (i % 5 == 0) {
        print "FizzBuzz";
      }
      else {
        print "Fizz";
      }
    }
    else {
      if (i % 5 == 0) {
        print "Buzz";
      } 
      else {
        print i;
      }
    }
  }
}

FizzBuzz(15);